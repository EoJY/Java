package homework;

public class Homework_p15 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//p15
		int a = 20, b = 10, c = 0;
		c = a+b;
		System.out.println(c);
		c = a-b;
		System.out.println(c);
		c = a*b;
		System.out.println(c);
		c = a/b;
		System.out.println(c);
		c = a%b;
		System.out.println(c);
		c = 12%5;
		System.out.println(c);
	}

}
