package if_ifelse;

public class P4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a= 5, b = 15, c = 15;	
		
		if (a > 10) 
		{
			System.out.println("a="+a);
			
		}
		if (b > 10)
		{
			System.out.println("b="+b);
		}
		if (c < 10)
		{
			System.out.println("c="+c);
		}
		
		else 
		{
			System.out.println("´Ù Æ²¸²");
			
		}
		
	
	}

}
